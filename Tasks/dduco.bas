DEF TOK(SSTR,ARG)  
IDX=0 
CSTR=""
LLEN=LEN(SSTR)-1
FOR ICT=0 TO LLEN
CV=MID(SSTR,ICT,1)
IF CV<>"," THEN 
CSTR=CSTR+CV 
ELSE  
IF IDX=ARG THEN
RETURN CSTR
ENDIF
CSTR="" 
IDX=IDX+1
ENDIF
NEXT ICT 
ENDDEF  
AS=TOK(INPUTPAYLOAD,0)
BS=TOK(INPUTPAYLOAD,1)
DF=TOK(INPUTPAYLOAD,2)
DT=TOK(INPUTPAYLOAD,3)
DIFF=VAL(DF) 
FDIFF=-1
FOR I=VAL(DF) TO VAL(DT)
CHECK = AS+STR(I)
RSHA = SHA1(CHECK)  
IF RSHA<>BS THEN
ELSE
 FDIFF = I 
 I = VAL(DT)
ENDIF
NEXT I
OUTPUTPAYLOAD=STR(FDIFF)

